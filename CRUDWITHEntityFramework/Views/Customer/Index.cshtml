@model IEnumerable<CRUDWITHEntityFramework.Models.Location>
@{
}
<table class="table table-bordered">
    <tr>
        <th>Locations</th>
        <th>Details</th>
    </tr>
    @foreach(var location in Model)
            {
                <tr>
                    <td>
                        <a asp-action="CustomerList" asp-controller="Customer" asp-rout-id="@location.Id">@location.Place</a>
                    </td>
                    <td>@location.Details</td>
                </tr>
            }

</table>